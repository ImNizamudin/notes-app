							"const response = pm.response.json();\r",
							"\r",
							"if (response.data && response.data.accessToken) {\r",
							"    pm.environment.set(\"ACCESS_TOKEN\", response.data.accessToken);\r",
							"    console.log(\"✅ ACCESS_TOKEN disimpan ke environment\");\r",
							"} else {\r",
							"    console.warn(\"❌ ACCESS_TOKEN tidak ditemukan di response!\");\r",
							"}\r",
							"\r",
							"if (response.data && response.data.refreshToken) {\r",
							"    pm.environment.set(\"REFRESH_TOKEN\", response.data.refreshToken);\r",
							"    console.log(\"✅ REFRESH_TOKEN disimpan ke environment\");\r",
							"} else {\r",
							"    console.warn(\"❌ REFRESH_TOKEN tidak ditemukan di response!\");\r",
							"}\r",
							""
